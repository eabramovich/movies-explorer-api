{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "import/no-extraneous-dependencies": ["off"],
    "import/extensions": ["error", "ignorePackages"],
    "arrow-body-style": ["error", "as-needed"],
    "no-useless-escape": ["off"],
    "no-unused-vars": ["off"]
  }
}